{
    "version": 2,
    "functions": {
        "api/index.php": {
            "runtime": "vercel-php@0.6.0"
        }
    },
    "routes": [
        {
            "src": "/(css|js|images|storage|vendor|build)/.*",
            "dest": "/public/$0"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ]
}
